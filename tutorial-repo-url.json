{
    "ActionList": {
      "start": {
        "Arguments": {
          "folder": "tutorial",
          "output1": "sample1.csv",
          "output2": "sample2.csv"
        },
        "InvokeNext": [
          "sum"
        ],
        "FaaSServer": "GH",
        "Type": "R",
        "FunctionName": "create_sample_data"
      },
      "sum": {
        "FunctionName": "compute_sum",
        "FaaSServer": "GH",
        "Type": "R",
        "Arguments": {
          "folder": "tutorial",
          "input2": "sample2.csv",
          "input1": "sample1.csv",
          "output": "sum.csv"
        },
        "InvokeNext": []
      }
    },
    "ComputeServers": {
      "GH": {
        "FaaSType": "GitHubActions",
        "UserName": "nolcut",
        "UseSecretStore": true,
        "ActionRepoName": "FaaSr-workflow-testing",
        "Branch": "main"
      }
    },
    "DataStores": {
      "S3": {
        "Endpoint": "https://play.min.io",
        "Bucket": "faasr",
        "Region": "us-east-1"
      }
    },
    "ActionContainers": {
      "start": "ghcr.io/nolcut/github-actions-r-testing:func-path",
      "sum": "ghcr.io/nolcut/github-actions-r-testing:func-path"
    },
    "FunctionInvoke": "start",
    "DefaultDataStore": "S3",
    "FunctionGitRepo": {
      "create_sample_data": "https://github.com/nolcut/FaaSr-Functions.git",
      "compute_sum": "https://github.com/nolcut/FaaSr-Functions.git"
    },
    "LoggingDataStore": "S3",
    "FaaSrLog": "FaaSrLog",
    "InvocationID": "tutorial-repo-url",
